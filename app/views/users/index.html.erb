<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<div>
  <% @users.each do |user| %>
    <p>Name</p>
    <ul>
      <li>
        <%= user.name %>
        <%= link_to "Delete", user_path(user), method: :delete %>
        <%= link_to "Edit", edit_user_path(user) %>
      </li>
    </ul>
  <% end %>
</div>

<%= link_to "Add New User", new_user_path %>
